@model List<tasktracker.Models.ShowDataBaseForHistoryModel>
<div class="container">
    <table class="table table-striped table-condensed table-bordered">
        <thead>
            <tr>
                <th>Заявка</th>
                <th>Статус</th>
                <th>Время</th>
                <th>Комментарий</th>
            </tr>
        </thead>
        <tbody>
            @for (int i = 0; i < Model.Count(); i++)
            {
                <tr>
                    <td>@Model[i].Story</td>
                    <td>@Model[i].Status</td>
                    <td>@Model[i].Time</td>
                    <td>@Model[i].Comment</td>
                </tr>
            }
        </tbody>
    </table>
</div>